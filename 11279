#include <iostream>
#include <algorithm>

using namespace std;

int heap[100001];
int hn;

int pop()
{
    int ans=heap[1];
    heap[1]=heap[hn];
    heap[hn--]=0;
    for(int i=1; i*2<=hn;)
    {
        if(heap[i]>heap[2*i] && heap[i]>heap[2*i+1])
            break;
        else if(heap[2*i]>heap[2*i+1])
        {
            swap(heap[i], heap[2*i]);
            i=2*i;
        }
        else
        {
            swap(heap[i], heap[2*i+1]);
            i=2*i+1;
        }
    }
    return ans;
}

void push(int x)
{
    heap[++hn]=x;
    for(int i=hn; i>1; i/=2)
    {
        if(heap[i]>heap[i/2])
            swap(heap[i], heap[i/2]);
        else
            break;
    }
}

int main()
{
    int n;
    scanf("%d", &n);
    while(n--)
    {
        int x;
        scanf("%d", &x);
        if(x==0)
        {
            if(hn==0)
                printf("0\n");
            else
                printf("%d\n", pop());
        }
        else
        {
            push(x);
        }
    }
}
